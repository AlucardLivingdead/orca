#: ../orca.desktop.in.h:1
msgid "Orca Screen Reader and Magnifier"
msgstr "Povečevalo in bralnik zaslona Orca"

#: ../orca.desktop.in.h:2
msgid "Present on-screen information as speech or braille, or magnify the screen"
msgstr "Vsebino zaslona prikaže v Braillovi pisavi ali jo prebere in povečuje zaslon"

#: ../src/orca/app_gui_prefs.py:102
#, python-format
msgid "Orca Preferences for %s"
